{
  "name": "recipehub-backend",
  "version": "1.0.0",
  "description": "Backend for RecipeHub",
  "license": "ISC",
  "author": "",
  "main": "index.ts",
  "scripts": {
    "test": "jest --silent",
    "test:debug": "jest",
    "start": "tsx index.ts",
    "dev": "tsx watch index.ts",
    "lint": "eslint .",
    "sequelize": "dotenv -e .env -- sh -c 'sequelize-auto -o ./src/models -d $DB_NAME -h $PG_HOST -u $PG_USER -p $PG_PORT -x $PG_PASSWORD -e postgres -l ts --sg --cm p --cf c --cp c'",
    "seed": "tsx ./db_setup_scripts/seed/runSeed.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.14.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "sequelize-auto": "^0.8.8",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
